{
  "version": "5.0.0-pre-01",
  "description": "TAP interop with EAP.",
  "authors": [ "Stephen Cleary" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
